<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="880.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.controller.MainController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="60.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <VBox prefHeight="560.0" prefWidth="60.0">
               <children>
                  <Button fx:id="searchBtn" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="search" text="Tra từ" />
                  <Button fx:id="translateBtn" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="translate" text="Dịch từ" />
                  <Button fx:id="gameBtn" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="game" text="Game" />
                  <Button fx:id="bookmarkBtn" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="save" text="Đã lưu" />
                  <Button fx:id="historyBtn" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="history" text="Lịch sử" />
                  <Button fx:id="settingBtn" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" styleClass="setting" text="Cài đặt" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="mainPane" layoutX="80.0" prefHeight="600.0" prefWidth="800.0" />
   </children>
</AnchorPane>
